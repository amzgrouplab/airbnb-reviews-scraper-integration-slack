      "editor": "textfield"
    },
    "proxyConfiguration": {
      "title": "Proxy configuration",
      "type": "object",
      "description": "Use Apify Proxy or your custom Proxy.",
      "prefill": {
        "useApifyProxy": true
      },
      "default": {
        "useApifyProxy": true
      },
      "editor": "proxy",
      "sectionCaption": "For developers"
    },
    "maxConcurrency": {
      "title": "Max concurrency",
      "type": "integer",
      "description": "Defines how many pages can be processed by the scraper in parallel. The scraper automatically increases and decreases concurrency based on available system resources. Use this option to set a hard limit.",
      "minimum": 1,
      "default": 50,
      "prefill": 50
    },
    "limitPoints": {
      "title": "Limit points",
      "type": "integer",
      "description": "Limits the number of geo points returned by the reverse API. The higher the number, the longer it takes. There's no upper limit.",
      "minimum": 1,
      "default": 100,
      "prefill": 100
    },
    "timeoutMs": {
      "title": "Timeout milliseconds",
      "type": "integer",
