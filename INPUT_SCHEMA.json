{
  "title": "Airbnb Scraper input",
  "description": "You can scrape Airbnb either by search results (Destination) or by a specific listing (link to a Place). To try it out, just type in your Destination, click &#9655;<b> Start </b> to start scraping! If you need any guidance, just <a href='https://blog.apify.com/how-to-scrape-airbnb-listings-and-reviews/' target='_blank' rel='noopener'>follow this tutorial</a>.",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "locationQuery": {
      "title": "Destination",
      "type": "string",
      "description": "Location name as you would search it on Airbnb.",
      "prefill": "Sacramento, California",
      "editor": "textfield"
    },
    "maxListings": {
      "title": "Number of results",
      "type": "integer",
      "description": "Limit the amount of results you want. Higher limits will take longer to scrape.",
      "prefill": 10,
      "example": 10
    },
    "simple": {
      "title": "Keep the dataset Excel friendly",
      "type": "boolean",
      "description": "The scraper will get only core listing data from the Airbnb search result page.",
      "default": true
    },
    "startUrls": {
      "title": "Link to an Airbnb Place",
      "type": "array",
      "description": "Link to a room, house, flat, or apartment. When using this field, the scraper will ignore the Destination field and will get results only for specific listings.",
      "prefill": [],
      "editor": "requestListSources",
      "sectionCaption": "Airbnb Places (not Destination)"
    },
    "includeReviews": {
      "title": "Include reviews",
      "type": "boolean",
      "description": "Do you want to scrape reviews as well?",
      "default": true,
      "sectionCaption": "Reviews, host info and availability"
    },
    "maxReviews": {
      "title": "Number of reviews",
      "type": "integer",
      "description": "Limit the number of reviews per listing. A higher number of reviews will take longer to scrape.",
      "prefill": 10
    },
    "calendarMonths": {
      "title": "Availability for next months",
      "type": "integer",
      "description": "Scrape the host's calendar to monitor the listing's availability for future months. E.g. set 12 to get data for the whole year, set 0 to skip the calendar altogether.",
      "default": 0,
      "prefill": 0,
      "minimum": 0,
      "maximum": 12
    },
    "addMoreHostInfo": {
      "title": "Include host info",
      "type": "boolean",
      "description": "Scrape primary host info such as host URL and number of listings. This works in sync with the <i> Keep the dataset Excel friendly </i> toggle &#8593;",
      "default": false
    },
